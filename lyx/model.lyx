\begin_inset Formula 
\begin{eqnarray}
a_{t+1} & = & (1+r(a_{t}))\left(a_{t}+y_{t}-c_{t}+(1-\phi-\kappa_{d})p_{dt}\right)\label{eq:budget-own-move}\\
a_{t+1} & \geq & 0\nonumber 
\end{eqnarray}

\end_inset


